<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <script src="https://cdn.tailwindcss.com"></script>


</head>
<body>

<h1 class="font-black text-6xl mx-auto w-full text-center my-10">Product manager</h1>
<!-- Container for demo purpose -->
<div class="container py-12 mx-auto px-4 md:px-6 lg:px-12 ">

    <!--Section: Design Block-->
    <section class="mb-20 text-gray-800">

        <div class="block rounded-lg shadow-lg ">
            <div class="flex flex-col">
                <div class="overflow-x-auto sm:-mx-6 lg:-mx-8">
                    <div class="inline-block min-w-full sm:px-6 lg:px-8">
                        <div class="overflow-hidden">
                            <table class="min-w-full mb-0">
                                <thead class="border-b rounded-t-lg text-left bg-blue-700">
                                <tr>
                                    <th scope="col" class="text-white rounded-tl-lg text-sm font-medium px-6 py-4">ProductCode</th>
                                    <th scope="col" class="text-white text-sm font-medium px-6 py-4">ProductName</th>
                                    <th scope="col" class="text-white text-sm font-medium px-6 py-4">ProductDate</th>
                                    <th scope="col" class="text-white text-sm font-medium px-6 py-4">ProductOriginPrice</th>
                                    <th scope="col" class="text-white text-sm font-medium px-6 py-4">Quantity</th>
                                    <th scope="col" class="text-white text-sm font-medium px-6 py-4">ProductStoreCode</th>
                                    <th scope="col" class="text-white rounded-tr-lg text-sm font-medium px-6 py-4"></th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr class="border-b">
                                    <th class="text-sm font-medium px-6 py-4 whitespace-nowrap text-left" scope="row">A12</th>
                                    <td class="text-sm font-normal px-6 py-4 whitespace-nowrap text-left text-gray-500">Product 1</td>
                                    <td class="text-sm font-normal px-6 py-4 whitespace-nowrap text-left text-gray-500">22/08/2023</td>
                                    <td class="text-sm font-normal px-6 py-4 whitespace-nowrap text-left text-gray-500">5.000.000</td>
                                    <td class="text-sm font-normal px-6 py-4 whitespace-nowrap text-left text-gray-500">1000</td>
                                    <td class="text-sm font-normal px-6 py-4 whitespace-nowrap text-left text-gray-500">S10</td>

                                    <td class="text-sm font-normal  py-4 whitespace-nowrap text-right ">
                                        <a href="#!" class="mr-4 bg-blue-600 text-white px-4 py-2 rounded font-medium text-blue-600">Add</a>
                                        <a href="#!" class="mr-4 bg-red-600 text-white px-4 py-2 rounded font-medium text-blue-600">Delete</a>
                                    </td>
                                </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </section>
    <!--Section: Design Block-->

</div>
<!-- Container for demo purpose -->

</body>
</html>
